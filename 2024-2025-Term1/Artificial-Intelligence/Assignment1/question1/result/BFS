// BFS
digraph {
	S [label=S color=red shape=circle]
	d [label=d color=black shape=circle]
	e [label=e color=red shape=circle]
	p [label=p color=black shape=circle]
	b [label=b color=black shape=circle]
	c [label=c color=black shape=circle]
	h [label=h color=black shape=circle]
	r [label=r color=red shape=circle]
	q [label=q color=black shape=circle]
	a [label=a color=black shape=circle]
	f [label=f color=red shape=circle]
	G [label=G color=red shape=circle]
	S -> d [color=black]
	S -> e [color=red]
	S -> p [color=black]
	d -> b [color=black]
	e -> c [color=black]
	e -> h [color=black]
	e -> r [color=red]
	p -> q [color=black]
	b -> a [color=black]
	r -> f [color=red]
	f -> G [color=red]
}
